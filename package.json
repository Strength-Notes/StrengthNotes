{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint App.js screens redux __tests__",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": ""
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-navigation|@expo(.*)|expo(.*)|@unimodules|redux-persist-expo-filesystem))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/material-top-tabs": "^5.2.10",
    "@react-navigation/native": "^5.4.3",
    "@react-navigation/stack": "^5.4.0",
    "array-move": "^3.0.0",
    "expo": "^48.0.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-calendars": "^1.301.0",
    "react-native-draggable-flatlist": "^2.3.3",
    "react-native-elements": "^2.0.3",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-material-menu": "^1.1.3",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "~2.15.0",
    "react-native-tab-view": "^2.14.4",
    "react-native-vector-icons": "^7.0.0",
    "react-native-web": "~0.13.12",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-filesystem": "^2.0.0",
    "expo-document-picker": "~8.4.1",
    "expo-file-system": "~9.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.2",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "jest": "latest",
    "jest-expo": "^40.0.0",
    "logkitty": "^0.7.1",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "yargs-parser": "^18.1.3"
  },
  "private": true
}
